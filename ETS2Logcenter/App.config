            </setting>
            <setting name="windowPositon" serializeAs="String">
                <value>400, 200</value>
            </setting>
            <setting name="windowSize" serializeAs="String">
                <value>700, 300</value>
            </setting>
            <setting name="chckCCase" serializeAs="String">
                <value>False</value>
            </setting>
            <setting name="txtSearchLine" serializeAs="String">
                <value />
